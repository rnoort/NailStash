<%= form_with model: brand do |form|%>
    <div class="form-group">
        <%= form.label :name %>
        <%= form.text_field :name %>
        <% brand.errors.full_messages_for(:name).each do |message| %>
            <div><%= message %></div>
        <% end %>
    </div>

    <%= form.submit %>
<% end %>